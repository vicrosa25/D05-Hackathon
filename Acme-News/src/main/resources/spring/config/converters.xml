<?xml version="1.0" encoding="UTF-8"?>

<!-- * converters.xml * -->

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />

		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />

		<bean class="converters.UsuarioToStringConverter" />
		<bean class="converters.StringToUsuarioConverter" />

		<bean class="converters.PeriodistaToStringConverter" />
		<bean class="converters.StringToPeriodistaConverter" />
		
		<bean class="converters.MangerToStringConverter" />
		<bean class="converters.StringToManagerConverter" />

		<bean class="converters.ModeradorToStringConverter" />
		<bean class="converters.StringToModeradorConverter" />

		<bean class="converters.NoticiaToStringConverter" />
		<bean class="converters.StringToNoticiaConverter" />

		<bean class="converters.InformacionToStringConverter" />
		<bean class="converters.StringToInformacionConverter" />

		<bean class="converters.ReporteToStringConverter" />
		<bean class="converters.StringToReporteConverter" />

		<bean class="converters.ComentarioToStringConverter" />
		<bean class="converters.StringToComentarioConverter" />

		<bean class="converters.TasaToStringConverter" />
		<bean class="converters.StringToTasaConverter" />

		<bean class="converters.PremioToStringConverter" />
		<bean class="converters.StringToPremioConverter" />

		<bean class="converters.ManagerToStringConverter" />
		<bean class="converters.StringToManagerConverter" />

		<bean class="converters.ConfigurationsToStringConverter" />
		<bean class="converters.StringToConfigurationsConverter" />

	</util:list>

</beans>